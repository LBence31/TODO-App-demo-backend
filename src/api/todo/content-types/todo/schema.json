{
  "kind": "collectionType",
  "collectionName": "todos",
  "info": {
    "singularName": "todo",
    "pluralName": "todos",
    "displayName": "Todo",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "description": {
      "type": "text",
      "required": true
    },
    "expirationDate": {
      "type": "date"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "done",
        "notdone",
        "archive"
      ],
      "required": true,
      "default": "notdone"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "todos"
    }
  }
}
